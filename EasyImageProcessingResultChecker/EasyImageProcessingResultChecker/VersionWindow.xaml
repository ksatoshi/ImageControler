<Window x:Class="EasyImageProcessingResultChecker.VersionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasyImageProcessingResultChecker"
        mc:Ignorable="d"
        Title="Version" Height="150" Width="250"
        ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.7*"/>
            <RowDefinition Height="0.3*"/>
        </Grid.RowDefinitions>
        <Button x:Name="obj_text_ver" Content="Button" HorizontalAlignment="Center" Grid.Row="1" VerticalAlignment="Center"/>
        <Label x:Name="obj_button_verok" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center" Content="aaa"/>

    </Grid>
</Window>
